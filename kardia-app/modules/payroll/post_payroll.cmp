		FROM
		    /apps/kardia/data/Kardia_DB/a_payroll_item/rows i,
		    /apps/kardia/data/Kardia_DB/a_payroll/rows y,
		    /apps/kardia/data/Kardia_DB/a_payroll_item_type/rows t,
