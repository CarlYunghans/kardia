TableRowHeights "application/filespec"
    {
    // General parameters.
    filetype = csv
    header_row = yes
    header_has_titles = no
    key_is_rowid = yes
    new_row_padding = 32

    // Column specifications.
    height "filespec/column" { type=integer id=1 }
    descrip "filespec/column" { type=string id=2 }
    fontsize "filespec/column" { type=integer id=3 }
    }

